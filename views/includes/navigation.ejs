<header>
    	<section>
		<div class="mm">
			<div class="mm-inn">
				<div class="mm-logo">
						<a href="/"><img style="width: 64px; height: 64px;" src="images/logo1.png" alt="" />
						</a> <span style="color: white; font-weight: 800; font-size: 1.7rem;">&nbsp;Panda Restaurant</span>
				</div>
				<div class="mm-icon"><span><i class="fa fa-bars show-menu" aria-hidden="true"></i></span>
				</div>
				<div class="mm-menu">
					<div class="mm-close"><span><i class="fa fa-times hide-menu" aria-hidden="true"></i></span>
					</div>
					<!-- <ul>
						<li><a href="/">Home</a>
						</li>
						<li><a href="/aboutus">About Us</a>
						</li>
						<li><a href="/contactus">Contact Us</a>
						</li>
					</ul> -->
					<ul>
							<!-- already login -->
							<% if (typeof isLogined !== 'undefined' && isLogined === true) { %>
								<li><a href="/">Welcome, <%= UserLastName %></a>
							</li>
                <% if(typeof userRole !== 'undefined' && userRole === '0' ) { %>
									<!-- admin menu -->
									<li><a href="/bookingform">Booking</a></li> 
                 <li><a href="/getAllBookings">Reservation Search</a></li>
									 <% }else { %>
											<!-- normal user menu -->
											<li><a href="/bookingform">Booking</a></li> 
										<% } %>
								<!-- not yet login -->
								<% } else { %>
									<li><a href="/register">Sign Up</a>
									</li>
									<li><a href="/login">Login</a>
									</li>
								<% } %>
									<li><a href="/aboutus">About Us</a>
									</li>
									<li><a href="/contactus">Contact Us</a>
									</li>
									<% if (typeof isLogined !== 'undefined' && isLogined === true) { %>
											<!-- log out -->
								 <li><a href="/logout">Logout</a>
									</li>
											<% } %>
						</ul>
				</div>
			</div>
		</div>
	</section>
	<!--HEADER SECTION-->
	<section class="main">
		<!--TOP SECTION-->
		<div class="menu-section">
			<div class="container">
				<div class="row">
						<div class="logo">
						<a href="/"><img style="width: 38px; height: 38px; margin: 10px;" src="images/logo1.png" alt="" />
						</a> <span style="color: white; width: 100%; font-weight: 800; font-size: 1.7rem;">Panda Restaurant</span>
					</div>
					<div class="top-bar">
						<ul>
							<!-- already login -->
							<% if (typeof isLogined !== 'undefined' && isLogined === true) { %>
								<li><a href="/">Welcome, <%= UserLastName %></a>
							</li>
                <% if(typeof userRole !== 'undefined' && userRole === '0' ) { %>
									<!-- admin menu -->
									 <li><a href="/bookingform">Booking</a></li> 
                 <li><a href="/getAllBookings">Reservation Search</a></li>
									 <% }else { %>
											<!-- normal user menu -->
											<li><a href="/bookingform">Booking</a></li> 
										<% } %>
								<!-- not yet login -->
								<% } else { %>
									<li><a href="/register">Sign Up</a>
									</li>
									<li><a href="/login">Login</a>
									</li>
								<% } %>
									<li><a href="/aboutus">About Us</a>
									</li>
									<% if (typeof isLogined !== 'undefined' && isLogined === true) { %>
											<!-- log out -->
								 <li><a href="/logout">Logout</a>
									</li>
											<% } %>
						</ul>
					</div>
				</div>
			</div>
		</div>
		<!--TOP SECTION-->
    </section>
</header>
